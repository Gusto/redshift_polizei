      <td><%= h header %></td>
    <% end %>
  </tr>
  <% @tables.each do |row| %>
    <tr>
      <% row.values.each do |value| %>
        <td>
          <%= h value %>
        </td>
      <% end %>
    </tr >
  <% end %>
</table>