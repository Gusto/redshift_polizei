<h2 name="queries" id="queries">Queries</h2>

<table class="table table-striped">
  <tr>
    <% @queries.first.keys.each do |header| %>
      <td><%= h header %></td>
    <% end %>
  </tr>
  <% @queries.each do |row| %>
    <tr>
      <% row.values.each do |value| %>
        <td>
          <%= h value %>
        </td>
      <% end %>
    </tr >
  <% end %>
</table>


<h2 name="tables" id="tables">Tables</h2>

<table class="table table-striped">
